<RelativeLayout android:layout_height="wrap_content" android:layout_width="match_parent" android:padding="16dp" xmlns:android="http://schemas.android.com/apk/res/android">
  
    
  <ImageView android:contentDescription="@null" android:id="@+id/nearby_urls_icon" android:layout_alignParentLeft="true" android:layout_alignParentTop="true" android:layout_height="16dp" android:layout_marginTop="4dp" android:layout_width="16dp">
    </ImageView>
  
    
  <TextView android:ellipsize="end" android:id="@+id/nearby_urls_title" android:layout_alignParentLeft="true" android:layout_alignParentTop="true" android:layout_height="wrap_content" android:layout_marginLeft="32dp" android:layout_width="match_parent" android:singleLine="true" android:textColor="@color/physical_web_header_color" android:textSize="16sp">
    </TextView>
  
    
  <TextView android:ellipsize="end" android:id="@+id/nearby_urls_url" android:layout_alignParentLeft="true" android:layout_below="@id/nearby_urls_title" android:layout_height="wrap_content" android:layout_marginLeft="32dp" android:layout_marginTop="4dp" android:layout_width="match_parent" android:singleLine="true" android:textColor="@color/physical_web_url_color" android:textSize="12sp">
    </TextView>
  
    
  <TextView android:ellipsize="end" android:id="@+id/nearby_urls_description" android:layout_alignParentLeft="true" android:layout_below="@id/nearby_urls_url" android:layout_height="wrap_content" android:layout_marginLeft="32dp" android:layout_marginTop="12dp" android:layout_width="match_parent" android:lineSpacingExtra="1.5dp" android:lines="2" android:textColor="@color/physical_web_url_description_color" android:textSize="14sp">
    </TextView>
  

</RelativeLayout>
